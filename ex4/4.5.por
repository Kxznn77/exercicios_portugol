programa {
  funcao inicio() {

    inteiro idade

    escreva("Qual sua idade?: ")
    leia(idade)

    cadeia gravidez

    escreva("Você está grávida? S/N: ")
    leia(gravidez)

    se (idade >= 65 ou gravidez == "S")
    escreva("Vá para a fila preferencial")

    senao
    escreva("Vá para a fila normal")
    
  }
}
